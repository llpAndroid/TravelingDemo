<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_location_detail"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.ptu.mata.view.activity.LocationDetail">


    <com.ptu.mata.widget.MyReFreshRecycleView
        android:id="@+id/recycler"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/iv_background">
    </com.ptu.mata.widget.MyReFreshRecycleView>

    <ImageView
        android:id="@+id/iv_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:src="@drawable/icon_destination_hotel_back"/>
</FrameLayout>
